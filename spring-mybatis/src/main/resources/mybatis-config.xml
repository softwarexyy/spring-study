<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
<!--        <setting name="logImpl" value="STDOUT_LOGGING"/>    &lt;!&ndash; 日志实现，打印mybatis的sql语句 &ndash;&gt;-->
        <setting name="mapUnderscoreToCamelCase" value="true"/> <!-- 开启驼峰命名变量和下划线字段的映射 -->
        <setting name="cacheEnabled" value="true"/>         <!-- 开启二级缓存 -->
    </settings>
    <!-- 别名设置 -->
    <typeAliases>
        <typeAlias type="com.yancy.entity.User" alias="user"></typeAlias>
    </typeAliases>

</configuration>